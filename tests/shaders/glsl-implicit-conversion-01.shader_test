[require]
GLSL >= 1.20

[vertex shader]
void main()
{
	gl_Position = gl_ModelViewProjectionMatrix * gl_Vertex;
}

[fragment shader]
#version 120
void main()
{
   int i = -5;
   gl_FragColor = i + vec4(5.0, 6.0, 5.0, 6.0);
}

[test]
clear color 0.0 0.0 0.0 0.0
clear
ortho
draw rect 10 10 10 10
probe rgb 15 15 0.0 1.0 0.0
